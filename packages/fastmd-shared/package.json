{
  "name": "@fastmd/shared",
  "version": "0.1.0",
  "description": "Shared types and protocol definitions for FastMD",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./protocol": {
      "types": "./dist/protocol.d.ts",
      "import": "./dist/protocol.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": ["fastmd", "types", "protocol", "rpc"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}
