{
  "name": "@example/minimal",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "biome check .",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "types:gen": "tsc -p tsconfig.types.json",
    "knip": "knip",
    "native:build": "bash -lc 'cd native/fastmd-native && pnpm --package=@napi-rs/cli@latest dlx napi build --release'",
    "native:test": "pnpm -C native/fastmd-native exec cargo test",
    "test:native": "FASTMD_NATIVE=1 bun test"
  },
  "dependencies": {
    "astro": "5.13.5",
    "cacache": "20.0.1",
    "find-cache-dir": "6.0.0",
    "gray-matter": "4.0.3",
    "json-stable-stringify": "1.3.0",
    "picomatch": "4.0.2",
    "pino": "9.5.0",
    "slash": "5.1.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "1.2.21",
    "@types/cacache": "19.0.0",
    "ajv": "^8.17.1",
    "bun-types": "1.2.21",
    "knip": "5.63.1",
    "typescript": "5.9.2"
  },
  "optionalDependencies": {
    "@fastmd/native": "workspace:*"
  },
  "packageManager": "pnpm@10.13.1"
}
